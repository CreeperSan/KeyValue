<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creepersan.keyvalue">

    <uses-permission android:name="android.permission.USE_FINGERPRINT"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.BootActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.TableAddActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.KeyValueAddActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.IconPickActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.RandomGenerateActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.ExportImportActivity" android:screenOrientation="userPortrait"/>
    </application>

</manifest>
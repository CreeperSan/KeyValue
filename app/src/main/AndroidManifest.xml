<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creepersan.keyvalue">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_app"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.BootActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.TableAddActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.IconPickActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.SettingActivity" android:screenOrientation="userPortrait"/>
        <activity android:name=".activity.AddKeyValueActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>